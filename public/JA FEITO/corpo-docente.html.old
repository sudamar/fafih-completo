    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Lato:wght@400;700&family=Montserrat:wght@700;800&display=swap" rel="stylesheet">
    <style>
        /* Estilos Unificados do index.html */
        :root { --primary-color: #0A2342; --secondary-color: #2C678F; --background-color: #F4F4F9; --text-color: #333; --card-bg: #FFFFFF; --shadow: 0 4px 12px rgba(0, 0, 0, 0.08); --gradient: linear-gradient(90deg, #6A0DAD, #0000FF, #008000, #FFFF00, #FFA500, #FF0000); --footer-blue: #1d4397; --card-green-dark: #006400; }
        * { margin: 0; padding: 0; box-sizing: border-box; }
        html { scroll-behavior: smooth; }
        body { font-family: 'Lato', sans-serif; background-color: var(--background-color); color: var(--text-color); line-height: 1.7; }
        
        header { background-color: var(--card-bg); padding: 0.25rem 4%; display: flex; justify-content: space-between; align-items: center; position: sticky; top: 0; z-index: 1000; box-shadow: var(--shadow); border-bottom: 4px solid; border-image: var(--gradient) 1; gap: 2rem; }
        .header-group-left { display: flex; align-items: center; gap: 20px; }
        .header-group-right { display: flex; align-items: center; gap: 1.5rem; }
        .logo-container { text-decoration: none; flex-shrink: 0; }
        .logo-img { height: 70px; }
        .logo-text-full { font-family: 'Montserrat', sans-serif; font-size: 0.9rem; color: var(--primary-color); font-weight: 700; line-height: 1.2; text-transform: uppercase; }
        
        nav { display: flex; align-items: center; }
        nav ul { list-style: none; display: flex; gap: 1.5rem; flex-wrap: wrap; align-items: center; padding: 0; margin: 0; }
        nav ul li { position: relative; }
        nav a { text-decoration: none; color: var(--primary-color); font-weight: 700; font-size: 0.95rem; position: relative; transition: color 0.3s ease; display: block; white-space: nowrap; }
        nav a:hover, nav a.active { color: var(--secondary-color); }
        
        .btn-inscreva-se { background-color: var(--footer-blue); color: white !important; padding: 0.8rem 2rem; border-radius: 50px; font-weight: 700; transition: background-color 0.3s ease, transform 0.3s ease; text-decoration: none; white-space: nowrap; }
        .btn-inscreva-se:hover { background-color: #2a5bbd; transform: scale(1.05); }
        
        .dropdown-menu { display: none; position: absolute; left: 0; top: 100%; background-color: var(--card-bg); list-style: none; padding: 0.75rem 0; margin-top: 0; border-radius: 5px; box-shadow: var(--shadow); min-width: 240px; border-top: 3px solid var(--primary-color); z-index: 100; }
        .dropdown-menu li { padding: 0; }
        .dropdown-menu a { padding: 0.3rem 1.5rem; font-size: 0.9rem; }
        nav ul li:hover .dropdown-menu { display: block; }
        .menu-toggle { display: none; cursor: pointer; background: none; border: none; padding: 0.5rem; }
        .menu-toggle .bar { display: block; width: 25px; height: 3px; margin: 5px auto; transition: all 0.3s ease-in-out; background-color: var(--primary-color); }

        section { padding: 5rem 8%; }
        .container { max-width: 1200px; margin: 0 auto; }
        h1, h2 { font-family: 'Montserrat', sans-serif; color: var(--primary-color); text-align: center; position: relative; }
        h1 { font-size: 2.5rem; margin-bottom: 1rem; }
        h2 { font-size: 2.5rem; margin-bottom: 4rem; }
        h2::after { content: ''; position: absolute; width: 80px; height: 4px; background: var(--gradient); bottom: -15px; left: 50%; transform: translateX(-50%); border-radius: 2px; }
        
        footer { background-color: var(--footer-blue); color: #e0e6f0; padding: 3rem 2% 1.5rem; margin-top: 0; }
        .footer-top-container { display: flex; justify-content: space-evenly; align-items: flex-start; flex-wrap: nowrap; width: 100%; max-width: 1800px; margin: 0 auto 2rem auto; }
        .footer-group-left { display: flex; align-items: flex-start; gap: 3rem; }
        .footer-col { flex-shrink: 1; }
        .footer-col.col-logo, .footer-col.col-emec { align-self: center; }
        .footer-col.col-logo { width: 320px; flex-shrink: 0; }
        .footer-col.col-emec { width: 280px; flex-shrink: 0; text-align: center; }
        .footer-logo-img, .footer-emec-img { max-width: 100%; height: auto; }
        .footer-col h4 { font-family: 'Montserrat', sans-serif; margin-bottom: 0.8rem; color: #fff; font-size: 1rem; white-space: nowrap; }
        .footer-col ul { list-style: none; padding: 0; }
        .footer-col li { margin-bottom: 0; white-space: nowrap; }
        .footer-col a, .footer-col .contact-item span { text-decoration: none; color: #e0e6f0; transition: color 0.3s ease; font-size: 0.8rem; line-height: 1.5; }
        .footer-col a:hover { color: #fff; }
        .social-icons { display: flex; gap: 1rem; margin-top: 1rem; }
        .social-icons a { display: inline-block; }
        .social-icons svg { width: 20px; height: 20px; fill: #e0e6f0; transition: fill 0.3s ease, transform 0.3s ease; }
        .contact-item { display: flex; align-items: center; gap: 10px; margin-bottom: 0; }
        .contact-item a { display: flex; align-items: center; gap: 10px; }
        .contact-item svg { width: 14px; height: 14px; fill: #e0e6f0; flex-shrink: 0; }
        .footer-bottom { text-align: center; margin-top: 1.5rem; padding-top: 1.5rem; border-top: 1px solid rgba(255, 255, 255, 0.2); color: rgba(255,255,255,0.7); }
        .footer-bottom p { font-size: 0.7rem; margin: 0; line-height: 1.4; }
        
        .modal { display: none; position: fixed; z-index: 2000; left: 0; top: 0; width: 100%; height: 100%; overflow: auto; background-color: rgba(0,0,0,0.6); }
        .modal-content { background-color: #fefefe; margin: 10% auto; padding: 30px; border: 1px solid #888; width: 80%; max-width: 600px; border-radius: 10px; position: relative; box-shadow: 0 5px 15px rgba(0,0,0,0.3); animation: fadeIn 0.5s; }
        .close-button { color: #aaa; float: right; font-size: 28px; font-weight: bold; position: absolute; top: 10px; right: 20px; }
        .close-button:hover, .close-button:focus { color: black; text-decoration: none; cursor: pointer; }
        .modal-content h2 { text-align: left; font-size: 1.8rem; margin-bottom: 1.5rem; color: var(--primary-color); }
        .modal-content h2::after { display: none; }
        .form-group { margin-bottom: 1rem; }
        .form-group label { display: block; margin-bottom: 0.5rem; font-weight: 700; color: #555; }
        .form-group input, .form-group select, .form-group textarea { width: 100%; padding: 0.8rem; border: 1px solid #ccc; border-radius: 5px; font-family: 'Lato', sans-serif; font-size: 1rem; }
        .form-group textarea { resize: vertical; min-height: 120px; }
        .btn-submit { width: 100%; background-color: var(--primary-color); color: white; padding: 1rem; border: none; border-radius: 5px; font-size: 1.1rem; font-weight: 700; cursor: pointer; transition: background-color 0.3s; margin-top: 1rem; }
        @keyframes fadeIn { from {opacity: 0} to {opacity: 1} }

        /* Estilos específicos da página */
        .page-section { padding: 4rem 5%; }
        .page-intro { font-size: 1.15rem; text-align: center; max-width: 900px; margin: 0 auto 2rem auto; color: #444; }
        .page-intro-secondary { font-size: 1.05rem; text-align: center; max-width: 900px; margin: 0 auto 4rem auto; color: #555; }
        .docente-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(350px, 1fr)); gap: 2rem; }
        .docente-card { background: var(--card-bg); border-radius: 10px; box-shadow: var(--shadow); display: flex; flex-direction: column; padding: 1.5rem; transition: transform 0.3s ease, box-shadow 0.3s ease; }
        .docente-card:hover { transform: translateY(-5px); box-shadow: 0 6px 15px rgba(0,0,0,0.1); }
        .docente-header { display: flex; align-items: center; gap: 1.5rem; margin-bottom: 1rem; }
        .docente-foto { width: 100px; height: 100px; border-radius: 50%; overflow: hidden; flex-shrink: 0; background-color: #e0e0e0; border: 3px solid var(--secondary-color); }
        .docente-foto img { width: 100%; height: 100%; object-fit: cover; }
        .docente-titulo h2 { font-size: 1.5rem; font-family: 'Montserrat', sans-serif; color: var(--primary-color); margin: 0; text-align: left; line-height: 1.3; }
        .docente-titulo h2::after { display: none; } /* Sobrescreve o estilo global */
        .docente-card p { font-size: 0.95rem; color: #555; margin-bottom: 0.5rem; }
        .docente-card p.contact-info { margin-top: 1rem; }
        .docente-card strong { color: var(--primary-color); }
        .docente-card a { color: var(--secondary-color); text-decoration: none; word-break: break-all; }
        .docente-card a:hover { text-decoration: underline; }
        .btn-container { text-align: center; margin-top: 4rem; }
        .btn-voltar { background-color: transparent; color: var(--secondary-color); border: 2px solid var(--secondary-color); padding: 0.8rem 2rem; text-decoration: none; border-radius: 50px; font-weight: 700; transition: all 0.3s ease; display: inline-block; }
        .btn-voltar:hover { background-color: var(--secondary-color); color: white; }

        @media (max-width: 1200px) { .footer-top-container { flex-wrap: wrap; justify-content: center; align-items: center; gap: 2rem; } .footer-group-left { flex-direction: column; align-items: center; text-align: center; gap: 2rem; } }
        @media (max-width: 992px) { 
            header { flex-wrap: wrap; }
            .header-group-right { display: none; flex-direction: column; width: 100%; background-color: var(--card-bg); position: absolute; top: 100%; left: 0; box-shadow: var(--shadow); padding: 1rem 0; }
            .header-group-right.nav-active { display: flex; }
            nav { width: 100%; }
            nav ul { flex-direction: column; width: 100%; gap: 0; }
            nav ul li { width: 100%; text-align: center; }
            nav a { padding: 1rem; border-bottom: 1px solid #eee; }
            .dropdown-menu { position: static; display: none; box-shadow: none; border: none; background-color: #f8f9fa; }
            nav ul li:hover .dropdown-menu { display: none; }
            nav ul li.open .dropdown-menu { display: block; }
            .btn-inscreva-se { margin-top: 1rem; width: fit-content; align-self: center; }
            .menu-toggle { display: block; }
            .menu-toggle.active .bar:nth-child(2) { opacity: 0; }
            .menu-toggle.active .bar:nth-child(1) { transform: translateY(8px) rotate(45deg); }
            .menu-toggle.active .bar:nth-child(3) { transform: translateY(-8px) rotate(-45deg); }
            .footer-top-container { flex-direction: column; text-align: center; }
            .footer-col li { white-space: normal; }
            .footer-col.col-logo, .footer-col.col-emec { width: 100%; max-width: 200px; margin: 0 auto; }
            .social-icons, .contact-item a, .contact-item { justify-content: center; }
        }
        @media (max-width: 768px) {
            h1 { font-size: 2rem; }
            .docente-header { flex-direction: column; text-align: center; }
            .docente-titulo h2 { text-align: center; }
        }
    </style>
</head>
